{
  "name": "adventuregamepage",
  "version": "1.0.0",
  "description": "AdventureGamePage admin/editor tooling and docs",
  "main": "scripts/admin.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "node scripts/run-tests.js",
    "test:manual": "node scripts/dev-server.js",
    "test:ci": "node scripts/dev-check.js && node scripts/run-tests.js --ci",
    "dev": "node scripts/dev-server.js",
    "check": "node scripts/dev-check.js",
    "lint": "eslint scripts/*.js",
    "format": "prettier --write \"scripts/*.js\" \"*.js\" \"*.json\"",
    "build": "echo 'No build process configured yet'",
    "docs": "echo 'Documentation generation not configured yet'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuShimoji/AdventureGamePage.git"
  },
  "keywords": [],
  "author": "AdventureGamePage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YuShimoji/AdventureGamePage/issues"
  },
  "homepage": "https://github.com/YuShimoji/AdventureGamePage#readme",
  "devDependencies": {
    "eslint": "^9.38.0",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  }
}
